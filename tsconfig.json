
{
    "compilerOptions": {
      "target": "ESNext",
      "module": "commonjs",
      "moduleResolution":"node",
      "strict": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "rootDir":"./src",
      "outDir": "dist"
    },
    "include": ["./src/**/*" ],
    "exclude": ["node_modules"],
    "lib":["es2015"]
}
